{
  "name": "wiprognosis-web-portal",
  "version": "0.5.1",
  "description": "Wiprognosis web portal v2, made of universal architecutre",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "clean": "rm -rf build",
    "copy": "better-npm-run copy",
    "bundle": "better-npm-run bundle_default && better-npm-run bundle_ECK",
    "client-build": "npm run bundle",
    "server-build": "npm run clean && npm run copy && better-npm-run server-build",
    "build": "npm run server-build && npm run client-build",
    "deploy": "pm2 start tools/server-preloader.js --no-daemon",
    "start-server": "better-npm-run start-server",
    "start-service": "better-npm-run start-service",
    "eslint": "better-npm-run eslint",
    "test": "better-npm-run test",
    "test-license": "better-npm-run test-license",
    "test-unit": "better-npm-run test-unit",
    "back-unit-db": "better-npm-run back-unit-db",
    "test-cover": "better-npm-run test-cover",
    "test-client": "jest --config jest.client.json",
    "test-obfuscator": "jest --config jest.obfuscator.json",
    "generate-jwt-keys": "gulp generate-jwt-keys",
    "generate-https-cert": "gulp generate-https-cert",
    "generate-license-pwd": "gulp generate-license-pwd",
    "generate-license-key": "babel-node tools/generate-license.js",
    "maya-jwt-keys": "gulp maya-jwt-keys",
    "update-license": "gulp generate-dev-license",
    "parse-word": "babel-node tools/words/word-parser.js",
    "dev-login": "gulp dev-login",
    "build-protected": "gulp javascript-obfuscator"
  },
  "betterScripts": {
    "dev": {
      "command": "gulp dev",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "bootstrap-dev"
      }
    },
    "copy": {
      "command": "mkdir build && cp -r src/app/public/ build/ && cp -r src/app/public/ build/public_ECK && cp -r src/app/public/ build/public_default"
    },
    "bundle_default": {
      "command": "webpack --bail --config tools/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "bundle-production",
        "IGNORE_KEY_FILE": true,
        "backgroundpic": "default"
      }
    },
    "bundle_ECK": {
      "command": "webpack --bail --config tools/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "bundle-production",
        "IGNORE_KEY_FILE": true,
        "backgroundpic": "ECK"
      }
    },
    "jest-base": {
      "command": "sh tools/test/jest.sh",
      "env": {
        "NODE_ENV": "test",
        "radiusip": "192.168.100.85",
        "radiusport": "10000",
        "systemdbip": "192.168.100.85",
        "systemdbport": "5432",
        "systemdbdatabase": "unittest",
        "systemdbname": "wiprognosis",
        "systemdbpw": "mysecretpassword",
        "mayadbip": "192.168.100.111",
        "mayadbport": "1433",
        "mayadbdatabase": "unittest",
        "mayadbname": "wi",
        "mayadbpw": "wistron",
        "mayasystemip":"192.168.100.111",
        "mayasystemport": "80",
        "mayasystemprotocol": "http",
        "universal": false
      }
    },
    "test": {
      "command": "better-npm-run jest-base",
      "env": {
        "PATTERN": "^((?!\\[license\\]).)*$",
        "TEST_TYPE": "api"
      }
    },
    "test-license": {
      "command": "better-npm-run jest-base",
      "env": {
        "PATTERN": "\\[license\\]",
        "TEST_TYPE": "api"
      }
    },
    "test-unit": {
      "command": "better-npm-run jest-base",
      "env": {
        "PATTERN": "\\[unit\\]",
        "TEST_TYPE": "unit"
      }
    },
    "back-unit-db": {
      "command": "babel-node tools/unittest/backcase.js",
      "env": {
        "mayadbip": "192.168.100.111",
        "mayadbport": "1433",
        "mayadbdatabase": "HD",
        "mayadbname": "wi",
        "mayadbpw": "wistron",
        "mayasystemip": "192.168.100.111",
        "mayasystemport": "80",
        "universal": false
      }
    },
    "test-cover": {
      "command": "better-npm-run jest-base",
      "env": {
        "PATTERN": "\\[unit\\]",
        "TEST_TYPE": "unit-cover"
      }
    },
    "server-build": {
      "command": "babel src -d build --copy-files ",
      "env": {
        "NODE_ENV": "production",
        "IGNORE_KEY_FILE": true
      }
    },
    "start-server": {
      "command": "node build/server/server-preloader.js",
      "env": {
        "NODE_ENV": "production",
        "radiusip": "192.168.100.85",
        "radiusport": "10000",
        "svip": "127.0.0.1",
        "universal": true,
        "asyncRendering": true
      }
    },
    "start-service": {
      "command": "node build/service/render-to-string",
      "env": {
        "NODE_ENV": "production",
        "universal": true,
        "svip": "127.0.0.1"
      }
    }
  },
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "universal",
    "node",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.mrd3.openlab.tw/ilun_chen/universal-webserver.git"
